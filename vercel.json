{
  "version": 2,
  "routes": [
    {
      "src": "/api",
      "dest": "/api/yiff.js"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:postId.gif",
      "destination": "/api?postId=:postId"
    },
    {
      "source": "/api/:postId.jpg",
      "destination": "/api?postId=:postId"
    },
    {
      "source": "/api/:postId.png",
      "destination": "/api?postId=:postId"
    }
  ]
}
